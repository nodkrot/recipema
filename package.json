{
  "name": "recipema",
  "version": "1.0.0",
  "description": "Family recipe hub.",
  "license": "MIT",
  "repository": "https://github.com/nodkrot/recipema",
  "author": "Vyacheslav Alayev",
  "scripts": {
    "start": "parcel ./app/index.html",
    "build": "parcel build app/index.html --no-source-maps --no-cache",
    "analyze": "parcel build ./app/index.html; open ./dist/report.html",
    "deploy": "./deploy.sh",
    "format": "prettier --write 'app/**/*.js'",
    "lint": "eslint 'app/**/*.js'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged; npm run lint;"
    }
  },
  "prettier": {
    "printWidth": 100,
    "trailingComma": "none"
  },
  "dependencies": {
    "antd": "^3.26.18",
    "browser-image-compression": "^1.0.12",
    "firebase": "^7.24.0",
    "fuse.js": "^6.4.2",
    "history": "^4.10.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-firebaseui": "^4.1.0",
    "react-router-cache-route": "^1.10.0",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "babel-eslint": "^10.1.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^7.12.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gh-pages": "^3.1.0",
    "husky": "^4.3.0",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0"
  }
}
