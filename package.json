{
  "name": "recipema",
  "version": "1.0.0",
  "description": "Family recipe hub.",
  "license": "MIT",
  "repository": "https://github.com/nodkrot/recipema",
  "author": "Vyacheslav Alayev",
  "scripts": {
    "start": "parcel ./app/index.html",
    "build": "parcel build app/index.html --no-source-maps --no-cache",
    "analyze": "parcel build ./app/index.html; open ./dist/report.html",
    "deploy": "./deploy.sh",
    "format": "prettier --write 'app/**/*.js'",
    "lint": "eslint 'app/**/*.js'"
  },
  "prettier": {
    "printWidth": 100,
    "trailingComma": "none"
  },
  "dependencies": {
    "antd": "^3.26.20",
    "browser-image-compression": "^1.0.14",
    "firebase": "^11.6.1",
    "fuse.js": "^6.4.6",
    "history": "^4.10.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-firebaseui": "^6.0.0",
    "react-router-cache-route": "^1.11.0",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-react": "^7.13.13",
    "babel-eslint": "^10.1.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^7.26.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gh-pages": "^3.1.0",
    "parcel-bundler": "^1.12.5",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "prettier": "^2.3.0"
  }
}
