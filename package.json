{
  "name": "recipema",
  "version": "1.0.0",
  "description": "Family recipe hub.",
  "license": "MIT",
  "repository": "https://github.com/nodkrot/recipema",
  "author": "Vyacheslav Alayev",
  "type": "module",
  "scripts": {
    "start": "parcel ./app/index.html",
    "build": "parcel build app/index.html --no-source-maps",
    "analyze": "parcel build ./app/index.html --reporter @parcel/reporter-bundle-analyzer",
    "deploy": "./deploy.sh",
    "format": "prettier --write 'app/**/*.js'",
    "lint": "eslint 'app/**/*.js'"
  },
  "prettier": {
    "printWidth": 100,
    "trailingComma": "none"
  },
  "dependencies": {
    "@ant-design/icons": "^6.1.0",
    "antd": "^5.22.5",
    "axios": "^1.9.0",
    "browser-image-compression": "^1.0.14",
    "firebase": "^12.7.0",
    "firestore-export-import": "^1.6.0",
    "fuse.js": "^6.4.6",
    "lodash": "^4.17.21",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^6.28.0",
    "react-router-dom": "^6.28.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.0",
    "@parcel/packager-raw-url": "^2.16.3",
    "@parcel/reporter-bundle-analyzer": "^2.16.3",
    "@parcel/transformer-webmanifest": "^2.16.3",
    "buffer": "^6.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^6.2.0",
    "parcel": "^2.16.3",
    "prettier": "^3.4.0"
  }
}
