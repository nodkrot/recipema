{
  "name": "recipema",
  "version": "1.0.0",
  "description": "Family recipe hub.",
  "license": "MIT",
  "repository": "https://github.com/nodkrot/recipema",
  "author": "Vyacheslav Alayev",
  "scripts": {
    "start": "parcel ./app/index.html",
    "build": "parcel build app/index.html --no-source-maps --no-cache",
    "analyze": "parcel build ./app/index.html; open ./dist/report.html",
    "deploy": "./deploy.sh",
    "format": "prettier --write 'app/**/*.js'",
    "lint": "eslint 'app/**/*.js'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged; npm run lint;"
    }
  },
  "prettier": {
    "printWidth": 100,
    "trailingComma": "none"
  },
  "dependencies": {
    "antd": "^3.26.15",
    "browser-image-compression": "^1.0.9",
    "firebase": "^7.14.0",
    "fuse.js": "^5.1.0",
    "history": "^4.10.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-firebaseui": "^4.1.0",
    "react-router-cache-route": "^1.8.4",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "babel-eslint": "^10.1.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "gh-pages": "^2.2.0",
    "husky": "^4.2.5",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "prettier": "^2.0.4",
    "pretty-quick": "^2.0.1"
  }
}
